{
  "name": "retyp-front",
  "description": "Retyp's front built with Nuxt & Tailwind CSS.",
  "author": "Arthur Dufour",
  "private": true,
  "version": "0.3.0",
  "keywords": [
    "retyp",
    "retyp-front"
  ],
  "scripts": {
    "dev": "./node_modules/.bin/nuxt",
    "build": "./node_modules/.bin/nuxt build",
    "start": "./node_modules/.bin/nuxt start",
    "lint": "./node_modules/.bin/eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "./node_modules/.bin/eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "test:e2e": "TEST=e2e ava --config ava.config.cjs ./tests/e2e/**/*"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/now-builder": "^0.17.2",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.3.2",
    "@nuxtjs/tailwindcss": "^2.0.0",
    "@nuxtjs/toast": "^3.3.0",
    "nuxt": "^2.13.3",
    "prismjs": "^1.20.0",
    "vue-prism-editor": "^0.6.1",
    "vue-shortkey": "^3.1.7"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@nuxtjs/eslint-config": "^2.0.2",
    "ava": "^3.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "husky": "^4.2.5",
    "jsdom": "^16.2.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
